create table reply_board(
board_num int,
board_name varchar2(20),
board_pass varchar2(15),
board_subject varchar2(50),
board_content varchar2(2000),
board_file varchar2(50),
board_re_ref int,
board_re_lev int,
board_re_seq int,
board_readcount int,
board_date date,
primary key(board_num)
);

db명 : reply_board
